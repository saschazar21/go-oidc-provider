{
  "client_name": "Acme Web Application",
  "client_description": "A demo web application for Acme Corp using OIDC authentication.",
  "client_uri": "https://app.acme-demo.com",
  "logo_uri": "https://app.acme-demo.com/logo.png",
  "grant_types": ["authorization_code", "implicit", "refresh_token"],
  "response_types": ["code", "code token"],
  "token_endpoint_auth_method": "client_secret_basic",
  "redirect_uris": [
    "https://minimal-client.example.com/callback",
    "https://app.acme-demo.com/callback",
    "https://app.acme-demo.com/auth/callback"
  ],
  "post_logout_redirect_uris": ["https://app.acme-demo.com/logout-success"],
  "require_auth_time": false,
  "require_pkce": true,
  "access_token_lifetime": 3600,
  "refresh_token_lifetime": 86400,
  "id_token_lifetime": 300,
  "is_active": true,
  "is_confidential": true
}
